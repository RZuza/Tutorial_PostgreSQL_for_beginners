CREATE DATABASE "EBank"
    WITH
    OWNER = postgres
    TEMPLATE = postgres
    ENCODING = 'UTF8'
    LC_COLLATE = 'Portuguese_Brazil.1252'
    LC_CTYPE = 'Portuguese_Brazil.1252'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1
    IS_TEMPLATE = True;

COMMENT ON DATABASE "EBank"
    IS 'Implementação de Banco de Dados Relacional - PostgreSQL';
